<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="36dp"
    android:minHeight="36dp">


    <TextView
        android:id="@+id/item_chatmanager_auto_complete_phonenumber"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:lines="1"
        android:layout_marginLeft="20dp"
        android:layout_marginStart="20dp"
        android:text="12345678910"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:maxLength="11"
        android:textSize="14sp"
        android:layout_centerVertical="true"/>

    <TextView
        android:id="@+id/item_chatmanager_auto_complete_nickname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="24dp"
        android:layout_marginStart="24dp"
        android:layout_marginRight="6dp"
        android:layout_marginEnd="6dp"
        android:layout_centerVertical="true"
        android:ellipsize="end"
        android:lines="1"
        android:textSize="14sp"
        android:layout_toEndOf="@id/item_chatmanager_auto_complete_phonenumber"
        android:layout_toRightOf="@id/item_chatmanager_auto_complete_phonenumber"
        android:text="音书新用户" />



</RelativeLayout>